config_version=5

[application]

config/name="Alien-attack"
run/main_scene="res://scenes/main_menu.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://assets/bitslix_splashscreen.png"
config/icon="res://Bitslix_logo_small.svg"
config/windows_native_icon="res://bitslix.ico"
boot_splash/minimum_display_time=2500

[autoload]

ShipStats="*res://scripts/ship_stats.gd"
Global="*res://autoloads/global.gd"
BackgroundMusic="*res://scenes/background_music.tscn"
ParticlesCache="*res://scripts/ParticlesCache.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/always_on_top=true
window/stretch/mode="canvas_items"

[editor]

movie_writer/movie_file="D:/godot/screencaptures/screencapture.avi"
movie_writer/disable_vsync=true

[editor_plugins]

enabled=PackedStringArray("res://addons/SimpleTODO/plugin.cfg", "res://addons/colorpicker_presets/plugin.cfg")

